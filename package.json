    "@nestjs/passport": "^7.1.5",
    "@nestjs/platform-express": "^7.6.15",
    "@types/dotenv": "^6.1.1",
    "@types/graphql-fields": "^1.3.3",
    "@types/mysql": "^2.15.18",
    "apollo-server-express": "^2.22.2",
    "core-js": "^3.9.1",
    "docxtemplater": "^3.21.1",
    "dotenv": "^8.2.0",
    "graphql": "^15.3.0",
    "graphql-fields": "^2.0.3",
    "graphql-scalars": "^1.9.0",
    "graphql-tools": "^4.0.8",
    "json-2-csv": "^3.10.2",
    "moment": "^2.29.1",
    "mysql": "^2.18.1",
    "node-geocoder": "^3.27.0",
    "passport": "^0.4.1",
    "passport-http-bearer": "^1.0.1",
    "passport-jwt": "^4.0.0",
    "pizzip": "^3.0.6",
    "prisma-binding": "^2.3.16",
    "reflect-metadata": "^0.1.12",
    "rimraf": "^2.7.1",
    "rxjs": "^6.6.7",
    "type-graphql": "^1.1.1",
    "typegraphql-nestjs": "^0.3.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.6.0",
    "@nestjs/testing": "^6.11.11",
    "@prisma/client": "^2.19.0",
    "@types/express": "^4.17.11",
    "@types/jest": "^23.3.13",
    "@types/node": "^10.17.56",
    "@types/supertest": "^2.0.10",
    "concurrently": "^4.1.2",
    "jest": "^23.6.0",
    "nodemon": "^1.19.4",
    "prettier": "^1.19.1",
    "prisma": "^2.19.0",
    "supertest": "^3.4.1",
    "ts-jest": "24.0.2",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "3.8.0",
    "tslint": "5.16.0",
    "typegraphql-prisma": "^0.12.0",
    "typescript": "^4.2.3",
    "wait-on": "^3.3.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },

}
